        <div class="col-sm-4 col-sm-offset-4 col-md-4 col-md-offset-4">

            <h4 class="text-dark text-center" style="margin-bottom:30px">@ViewBag.MessageText</h4>

        </div>

        <div class="form-group" style="margin-bottom:250px; margin-top:50px">
